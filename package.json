    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asyncy/components.git"
  },
  "files": [
    "dist",
    "src/scss"
  ],
  "homepage": "https://github.com/asyncy/components#readme",
  "style": "dist/asyncy-vue.css",
  "sass": "src/scss/asyncy.scss",
